{
  "cli": {
    "version": ">= 0.56.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "latest",
        "buildConfiguration": "Debug",
        "resourceClass": "m1-medium"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "image": "latest",
        "resourceClass": "m1-medium"
      }
    },
    "preview": {
      "distribution": "store",
      "ios": {
        "image": "latest",
        "buildConfiguration": "Debug",
        "resourceClass": "m1-medium"
      },
      "autoIncrement": true
    },
    "production": {
      "distribution": "store",
      "ios": {
        "image": "latest",
        "buildConfiguration": "Release",
        "resourceClass": "m1-medium"
      },
      "autoIncrement":true
    }
  },
  "submit": {
    "production": {}
  }
}
